public class GitHub {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a= scanner.nextInt();
        int b= scanner.nextInt();
        int kichikSon;
        int tartibRaqam;

        if(a<b){
            kichikSon=a;
            tartibRaqam=1;
        }else{
            kichikSon=b;
            tartibRaqam=2;
        }
        System.out.println("Kichik son: "+kichikSon);
        System.out.println("Kichik sonning tartib raqami: "+tartibRaqam);
    }
}
