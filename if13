 public class GitHub {
        public static void main(String[] args) {
            Scanner scanner = new Scanner(System.in);
            int x = scanner.nextInt();
            int y = scanner.nextInt();
            int z = scanner.nextInt();
            if(x > y && y > z){
                System.out.println(y);
            } else if (y > x && x > z) {
                System.out.println(x);
            } else{
                System.out.println(z);
            }
        }
    }
