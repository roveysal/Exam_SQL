public class GitHub {
    public static void main(String[] args) {
        System.out.println("Hajmi:" + volume(6));
        System.out.println("To'la sirti yuzasi:" + fullSurface(6));

    }

    public static int volume(int a) {
        return a * a * a;
    }
    public static int fullSurface(int a){
        return 6*a*a;
    }


}
